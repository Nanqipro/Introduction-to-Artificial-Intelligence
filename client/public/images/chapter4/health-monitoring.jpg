<svg xmlns="http://www.w3.org/2000/svg" width="400" height="300" viewBox="0 0 400 300">
  <defs>
    <linearGradient id="watchGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#333;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#666;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="heartGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#E91E63;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#C2185B;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="chartGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4CAF50;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#2196F3;stop-opacity:0.8" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="300" fill="#f8f9fa"/>
  
  <!-- Smart watch -->
  <g transform="translate(80, 100)">
    <!-- Watch band -->
    <rect x="-10" y="-30" width="20" height="60" fill="#555" rx="10"/>
    <rect x="-10" y="30" width="20" height="60" fill="#555" rx="10"/>
    
    <!-- Watch body -->
    <rect x="-25" y="-25" width="50" height="50" fill="url(#watchGradient)" rx="8"/>
    
    <!-- Watch screen -->
    <rect x="-20" y="-20" width="40" height="40" fill="#000" rx="4"/>
    
    <!-- Heart rate display -->
    <g transform="translate(0, -8)">
      <!-- Heart icon -->
      <path d="M -6 -2 Q -8 -6 -4 -6 Q 0 -6 0 -2 Q 0 -6 4 -6 Q 8 -6 6 -2 Q 0 4 0 4 Q 0 4 -6 -2" 
            fill="url(#heartGradient)"/>
      
      <!-- Heart rate number -->
      <text x="0" y="12" text-anchor="middle" font-family="Arial" font-size="8" font-weight="bold" fill="#4CAF50">72</text>
      <text x="0" y="20" text-anchor="middle" font-family="Arial" font-size="4" fill="#888">BPM</text>
    </g>
  </g>
  
  <!-- Health data visualization -->
  <g transform="translate(180, 50)">
    <!-- Chart background -->
    <rect x="0" y="0" width="180" height="120" fill="white" stroke="#ddd" stroke-width="2" rx="8"/>
    
    <!-- Chart title -->
    <text x="90" y="20" text-anchor="middle" font-family="Arial" font-size="12" font-weight="bold" fill="#333">健康监测数据</text>
    
    <!-- Heart rate chart -->
    <g transform="translate(20, 40)">
      <text x="0" y="0" font-family="Arial" font-size="10" fill="#666">心率</text>
      <path d="M 0 10 L 20 8 L 40 12 L 60 6 L 80 10 L 100 8 L 120 11 L 140 7" 
            stroke="#E91E63" stroke-width="3" fill="none"/>
      
      <!-- Heart rate points -->
      <circle cx="20" cy="18" r="2" fill="#E91E63"/>
      <circle cx="60" cy="16" r="2" fill="#E91E63"/>
      <circle cx="100" cy="18" r="2" fill="#E91E63"/>
      <circle cx="140" cy="17" r="2" fill="#E91E63"/>
    </g>
    
    <!-- Steps chart -->
    <g transform="translate(20, 70)">
      <text x="0" y="0" font-family="Arial" font-size="10" fill="#666">步数</text>
      <rect x="10" y="5" width="8" height="15" fill="#4CAF50"/>
      <rect x="25" y="8" width="8" height="12" fill="#4CAF50"/>
      <rect x="40" y="3" width="8" height="17" fill="#4CAF50"/>
      <rect x="55" y="6" width="8" height="14" fill="#4CAF50"/>
      <rect x="70" y="4" width="8" height="16" fill="#4CAF50"/>
      <rect x="85" y="7" width="8" height="13" fill="#4CAF50"/>
      <rect x="100" y="5" width="8" height="15" fill="#4CAF50"/>
    </g>
  </g>
  
  <!-- Health metrics panel -->
  <g transform="translate(50, 200)">
    <rect x="0" y="0" width="300" height="80" fill="white" stroke="#ddd" stroke-width="2" rx="8"/>
    
    <!-- Metrics grid -->
    <g transform="translate(20, 20)">
      <!-- Heart rate -->
      <g>
        <circle cx="15" cy="15" r="12" fill="#FFE0E6" stroke="#E91E63" stroke-width="2"/>
        <path d="M 9 12 Q 11 8 15 12 Q 19 8 21 12 Q 15 18 15 18 Q 15 18 9 12" fill="#E91E63"/>
        <text x="40" y="12" font-family="Arial" font-size="10" font-weight="bold" fill="#333">心率</text>
        <text x="40" y="22" font-family="Arial" font-size="12" font-weight="bold" fill="#E91E63">72 BPM</text>
      </g>
      
      <!-- Steps -->
      <g transform="translate(100, 0)">
        <circle cx="15" cy="15" r="12" fill="#E8F5E8" stroke="#4CAF50" stroke-width="2"/>
        <path d="M 10 18 L 12 15 L 15 18 L 18 12 L 20 15" stroke="#4CAF50" stroke-width="2" fill="none"/>
        <text x="40" y="12" font-family="Arial" font-size="10" font-weight="bold" fill="#333">步数</text>
        <text x="40" y="22" font-family="Arial" font-size="12" font-weight="bold" fill="#4CAF50">8,542</text>
      </g>
      
      <!-- Sleep -->
      <g transform="translate(200, 0)">
        <circle cx="15" cy="15" r="12" fill="#E3F2FD" stroke="#2196F3" stroke-width="2"/>
        <path d="M 12 10 Q 15 8 18 10 Q 15 12 12 10" fill="#2196F3"/>
        <circle cx="11" cy="16" r="1" fill="#2196F3"/>
        <circle cx="15" cy="18" r="1.5" fill="#2196F3"/>
        <circle cx="19" cy="16" r="1" fill="#2196F3"/>
        <text x="40" y="12" font-family="Arial" font-size="10" font-weight="bold" fill="#333">睡眠</text>
        <text x="40" y="22" font-family="Arial" font-size="12" font-weight="bold" fill="#2196F3">7.5h</text>
      </g>
    </g>
    
    <!-- Health status -->
    <g transform="translate(20, 50)">
      <text x="0" y="0" font-family="Arial" font-size="10" font-weight="bold" fill="#333">健康状态: </text>
      <text x="60" y="0" font-family="Arial" font-size="10" fill="#4CAF50">良好</text>
      <text x="120" y="0" font-family="Arial" font-size="9" fill="#666">建议: 保持当前运动量，注意休息</text>
    </g>
  </g>
  
  <!-- Wireless connection indicators -->
  <g transform="translate(150, 120)">
    <path d="M 0 0 Q 10 -5 20 0" stroke="#2196F3" stroke-width="2" fill="none" opacity="0.6"/>
    <path d="M 5 5 Q 10 2 15 5" stroke="#2196F3" stroke-width="2" fill="none" opacity="0.8"/>
    <circle cx="10" cy="8" r="2" fill="#2196F3"/>
  </g>
  
  <!-- Title -->
  <text x="200" y="30" text-anchor="middle" font-family="Arial" font-size="18" font-weight="bold" fill="#333">智能健康监测</text>
</svg>